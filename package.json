{
  "name": "koa-web-kit",
  "version": "2.0.0",
  "description": "Node koa web starter kit for quick development",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "deploy": "./deploy.sh",
    "build": "webpack --progress",
    "watch": "webpack --watch --progress",
    "prod": "cross-env NODE_ENV=production webpack -p --progress",
    "lint": "eslint --ext .js --cache .",
    "es6-module-to-commonjs": "babel test/es6/modules -d build/test/es6/modules --plugins transform-es2015-modules-commonjs",
    "test": "mocha ./test/mocha"
  },
  "engines": {
    "node": ">= 7.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonBoy/koa-web-kit.git"
  },
  "keywords": [
    "koa",
    "es6",
    "webpack",
    "sass",
    "react",
    "vue",
    "bootstrap"
  ],
  "author": "jasonlikenfs@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JasonBoy/koa-web-kit/issues"
  },
  "homepage": "https://github.com/JasonBoy/koa-web-kit#readme",
  "readme": "README.md",
  "dependencies": {
    "bluebird": "3.4.7",
    "bootstrap": "4.0.0-alpha.6",
    "consolidate": "0.14.5",
    "dateformat": "2.0.0",
    "koa": "2.0.1",
    "koa-bodyparser": "2.3.0",
    "koa-compress": "2.0.0",
    "koa-logger": "2.0.1",
    "koa-mount": "1.3.0",
    "koa-router": "next",
    "koa-session": "3.4.0",
    "koa-static": "3.0.0",
    "koa-views": "5.2.1",
    "lodash": "4.17.4",
    "nunjucks": "3.0.0",
    "request": "2.79.0",
    "request-debug": "0.2.0",
    "vue": "2.2.1",
    "vue-router": "2.3.0",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "autoprefixer": "6.7.5",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.23.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-1": "6.22.0",
    "babel-preset-stage-2": "6.22.0",
    "babel-preset-stage-3": "6.22.0",
    "chai": "3.5.0",
    "cross-env": "^3.1.4",
    "css-loader": "0.26.2",
    "del": "2.2.2",
    "eslint": "3.16.1",
    "eslint-loader": "1.6.3",
    "eslint-plugin-html": "2.0.1",
    "express": "4.14.1",
    "extract-text-webpack-plugin": "2.0.0",
    "file-loader": "0.10.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "image-webpack-loader": "3.2.0",
    "mocha": "3.2.0",
    "node-sass": "4.5.0",
    "postcss-loader": "1.3.2",
    "sass-loader": "6.0.1",
    "style-loader": "0.13.2",
    "uglify-loader": "1.4.0",
    "url-loader": "0.5.8",
    "vue-loader": "11.1.3",
    "vue-template-compiler": "2.2.1",
    "webpack": "2.2.1",
    "webpack-manifest-plugin": "^1.1.0"
  }
}
