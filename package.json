{
  "name": "koa-web-kit",
  "version": "2.0.0",
  "description": "Node koa web starter kit for quick development",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "deploy": "cross-env NODE_ENV=production ./deploy.sh",
    "build": "webpack --progress --config config/webpack.config.dev.js",
    "watch": "webpack --watch --progress --config config/webpack.config.dev.js",
    "prod": "cross-env NODE_ENV=production webpack -p --progress --config config/webpack.config.prod.js",
    "lint": "eslint --ext .js --cache .",
    "es6-module-to-commonjs": "babel test/es6/modules -d build/test/es6/modules --plugins transform-es2015-modules-commonjs",
    "test": "./node_modules/.bin/_mocha ./test/mocha"
  },
  "engines": {
    "node": ">= 7.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonBoy/koa-web-kit.git"
  },
  "keywords": [
    "koa",
    "es6",
    "webpack",
    "sass",
    "react",
    "vue",
    "bootstrap"
  ],
  "author": "jasonlikenfs@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JasonBoy/koa-web-kit/issues"
  },
  "homepage": "https://github.com/JasonBoy/koa-web-kit#readme",
  "readme": "README.md",
  "dependencies": {
    "bluebird": "3.5.0",
    "bootstrap": "4.0.0-alpha.6",
    "consolidate": "0.14.5",
    "dateformat": "2.0.0",
    "koa": "2.2.0",
    "koa-better-body": "3.0.2",
    "koa-compress": "2.0.0",
    "koa-morgan": "1.0.1",
    "koa-mount": "3.0.0",
    "koa-router": "7.0.1",
    "koa-session": "5.0.0",
    "koa-static": "3.0.0",
    "koa-views": "6.0.2",
    "lodash": "4.17.4",
    "nunjucks": "3.0.0",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "request": "2.81.0",
    "request-debug": "0.2.0",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "autoprefixer": "7.0.1",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "chai": "3.5.0",
    "compression-webpack-plugin": "0.4.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "4.0.0",
    "css-loader": "0.28.1",
    "del": "2.2.2",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-react": "7.0.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "image-webpack-loader": "3.3.0",
    "mocha": "3.3.0",
    "node-sass": "4.5.2",
    "postcss-loader": "2.0.0",
    "sass-loader": "6.0.3",
    "style-loader": "0.17.0",
    "url-loader": "0.5.8",
    "webpack": "2.5.1",
    "webpack-manifest-plugin": "1.1.0",
    "webpack-merge": "4.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 5 versions"
  ]
}
