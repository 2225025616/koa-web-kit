{
  "name": "koa-web-kit",
  "version": "1.0.0",
  "description": "Node koa web starter kit for quick development",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "deploy": "./deploy.sh",
    "build": "webpack --progress",
    "watch": "webpack --watch --progress",
    "prod": "webpack -p --progress",
    "lint": "eslint --ext .js --cache .",
    "es6-module-to-commonjs": "babel test/es6/modules -d build/test/es6/modules --plugins transform-es2015-modules-commonjs",
    "test": "mocha ./test/mocha"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonBoy/koa-web-kit.git"
  },
  "keywords": [
    "koa",
    "es6",
    "webpack",
    "sass",
    "react",
    "vue",
    "bootstrap"
  ],
  "author": "jasonlikenfs@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JasonBoy/koa-web-kit/issues"
  },
  "homepage": "https://github.com/JasonBoy/koa-web-kit#readme",
  "readme": "README.md",
  "dependencies": {
    "bluebird": "3.4.6",
    "bootstrap": "4.0.0-alpha.5",
    "consolidate": "0.14.5",
    "dateformat": "2.0.0",
    "koa": "1.2.4",
    "koa-bodyparser": "2.3.0",
    "koa-compress": "1.0.9",
    "koa-logger": "1.3.0",
    "koa-mount": "1.3.0",
    "koa-router": "5.4.0",
    "koa-session": "3.4.0",
    "koa-static": "2.0.0",
    "koa-views": "4.1.0",
    "lodash": "4.17.2",
    "nunjucks": "3.0.0",
    "request": "2.79.0",
    "request-debug": "0.2.0",
    "vue": "2.1.3",
    "vue-router": "2.0.3",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "6.5.3",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.8",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-1": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-preset-stage-3": "6.17.0",
    "chai": "3.5.0",
    "chunk-manifest-webpack-plugin": "0.1.0",
    "css-loader": "0.26.0",
    "del": "2.2.2",
    "eslint": "3.11.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-html": "1.7.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.1",
    "image-webpack-loader": "3.0.0",
    "mocha": "3.2.0",
    "node-sass": "3.13.0",
    "postcss-loader": "0.13.0",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "uglify-loader": "1.3.0",
    "url-loader": "0.5.7",
    "vue-loader": "10.0.1",
    "webpack": "1.13.3",
    "webpack-md5-hash": "0.0.5",
    "vue-template-compiler": "2.1.3"
  }
}
